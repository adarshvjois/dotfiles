    name = Adarsh Jois
    email = adarsh.jois@openspace.ai

[core]
	excludesfile = ~/.gitignore_global
[pull]
	rebase = true
[fetch]
	prune = true
[diff]
	colorMoved = zebra
[help]
    autoCorrect = immediate
[alias]
    lastdiff = diff HEAD^1..HEAD
    co = checkout
    br = branch
    cm = commit
    st = status
